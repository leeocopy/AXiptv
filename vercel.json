{
    "functions": {
        "api/proxy.cjs": {
            "runtime": "@vercel/node@3"
        }
    },
    "rewrites": [
        {
            "source": "/api/proxy",
            "destination": "/api/proxy.cjs"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ]
}